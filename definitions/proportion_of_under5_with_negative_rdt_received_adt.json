{
  "description": "Proportion of Under-5's with negative RDT result who received antimalarial/ADT medication",
  "name": "proportion_of_under5_with_negative_rdt_received_adt",
  "type": "Malaria",
  "value": {
    "proportion": {
      "numerator": [
        {
	  "calculation": 
            {
              "formula": "(meta_timeend - dob)/86400", 
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "patient_available_immediate_danger_sign": "fever",
            "patient_available_referral_follow_on_rdt_result": "negative",
            "patient_available_medication_type": "coartem"
          },
          "source": "Child List Visit.csv"
        }
      ],
      "denominator": [
        {
	  "calculation": 
            {
              "formula": "(meta_timeend - dob)/86400", 
              "name": "num_of_days"
          },
          "count": true, 
          "distinct": "case__case_id", 
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "patient_available_immediate_danger_sign": "fever",
            "patient_available_referral_follow_on_rdt_result": "negative"
          },
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}
