{
  "description": "Proportion of Under-5's with uncomplicated fever who did NOT receive RDT test due to 'RDT not available' with CHW",
  "name": "proportion_of_under5_with_uncomplecated_fever_no_rdt",
  "type": "Malaria",
  "value": {
    "proportion": {
      "numerator": [
        {
	  "calculation":
            {
              "formula": "(meta_timeend - dob)/86400",
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "server_computed__mvp_indicators_immediate_danger_sign_value": "fever",
            "$or": [
                {"server_computed__mvp_indicators_rdt_result_value": {"$regex": "rdt_not_available", "$options": "i"}},
                {"server_computed__mvp_indicators_rdt_result_value": {"$regex":"rdt_not_conduct", "$options": "i"}}
            ]
          },
          "source": "Child List Visit.csv"
        }
      ],
      "denominator": [
        {
	  "calculation":
            {
              "formula": "(meta_timeend - dob)/86400",
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "server_computed__mvp_indicators_immediate_danger_sign_value": "fever"
          },
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}
