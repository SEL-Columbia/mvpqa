{
  "description": "Number of Over-5 (non-maternal) deaths during the time period", 
  "name": "number_of_over_5_deaths", 
  "value": {
    "sum": [
      {
        "count": true, 
        "dataset_id": "0079a17a335e48e09dd63c8359f88cd9", 
        "query": {
          "form_age": {
            "$gt": 5
          },
          "form_date_of_death": {
            "$gte": "{{period_start}}",
            "$lt": "{{period_end}}"
          },
          "form_dob_known": "no"
        }, 
        "source": "Death without Registration.csv"
      }, 
      {
        "calculation": {
            "name": "calculated_age",
            "formula": "((form_date_of_death - form_dob)/86400)/365"
        },
        "count": true, 
        "dataset_id": "0079a17a335e48e09dd63c8359f88cd9", 
        "query": {
          "calculated_age": {
            "$gt": 5
          },
          "form_date_of_death": {
            "$gte": "{{period_start}}",
            "$lt": "{{period_end}}"
          },
          "form_dob_known": "yes"
        }, 
        "source": "Death without Registration.csv"
      }
    ]
  }
}
