{
  "description": "Proportion of Under-5's with uncomplicated diarrhea who recieved ORS",
  "name": "proportion_of_under5_with_uncomplicated_diarrhea_received_ors",
  "type": "Child Health",
  "value": {
    "proportion": {
      "denominator": [
        {
	  "calculation":
            {
              "formula": "(form_meta_timeend - computed__mvp_indicators_child_dob_value)/86400",
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "0079a17a335e48e09dd63c8359f88cd9",
          "query": {
            "form_meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "computed__mvp_indicators_immediate_danger_sign_value": "diarrhea"
          },
          "source": "Child List Visit.csv"
        }
      ],
      "numerator": [
        {
	  "calculation":
            {
              "formula": "(form_meta_timeend - computed__mvp_indicators_child_dob_value)/86400",
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "0079a17a335e48e09dd63c8359f88cd9",
          "query": {
            "form_meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "computed__mvp_indicators_immediate_danger_sign_value": "diarrhea",
            "computed__mvp_indicators_diarrhea_medication_value": {
                "$regex": "ors",
                "$options": "i"
            }
          },
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}

