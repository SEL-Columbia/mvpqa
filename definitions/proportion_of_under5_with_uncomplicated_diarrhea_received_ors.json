{
  "description": "Proportion of Under-5's with uncomplicated diarrhea who recieved ORS", 
  "name": "proportion_of_under5_with_uncomplicated_diarrhea_received_ors",
  "type": "Child Health",
  "value": {
    "proportion": {
      "denominator": [
        {
	  "calculation": 
            {
              "formula": "(meta_timeend - dob)/86400", 
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "0079a17a335e48e09dd63c8359f88cd9", 
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}", 
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "patient_available_immediate_danger_sign": "diarrhea"
          }, 
          "source": "Child List Visit.csv"
        }
      ], 
      "numerator": [
        {
	  "calculation": 
            {
              "formula": "(meta_timeend - dob)/86400", 
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "0079a17a335e48e09dd63c8359f88cd9", 
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}", 
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "patient_available_immediate_danger_sign": "diarrhea",
            "patient_available_medication_type": {
              "$in": ["ors", "ors zinc"]
            }
          }, 
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}

