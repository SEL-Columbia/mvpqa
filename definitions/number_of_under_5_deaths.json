        },
        "count": true,
        "distinct": "form_case__case_id",
        "dataset_id": "22af70b95bd84fa6a02098de19ac0304",
        "query": {
          "form_meta_timeend": {
            "$gte": "{{period.start}}",
            "$lte": "{{period.end}}"
