{
  "description": "Proportion of Under-5's with uncomplicated fever who recieved RDT test",
  "name": "proportion_of_under5_with_uncomplecated_fever_received_rdt",
  "type": "Malaria",
  "value": {
    "proportion": {
      "numerator": [
        {
	  "calculation": 
            {
              "formula": "(meta_timeend - dob)/86400", 
              "name": "num_of_days"
          },
          "count": true,
          "distinct": "case__case_id",
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "patient_available_immediate_danger_sign": "fever",
            "$or": [
              {
                "patient_available_referral_follow_on_rdt_result": "negative"
              },
              {
                "patient_available_referral_follow_on_rdt_result": "positive"
              }
            ]
          },
          "source": "Child List Visit.csv"
        }
      ],
      "denominator": [
        {
	  "calculation": 
            {
              "formula": "(meta_timeend - dob)/86400", 
              "name": "num_of_days"
          },
          "count": true,
          "distinct": "case__case_id",
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "patient_available_immediate_danger_sign": "fever"
          },
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}
