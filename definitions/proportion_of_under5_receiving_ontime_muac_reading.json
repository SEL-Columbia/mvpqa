{
  "description": "Proportion of children aged 6-59 months receiving on-time routine (every 90 days) MUAC readings during the time period",
  "name": "proportion_of_under5_receiving_ontime_muac_reading",
  "type": "Child Health",
  "value": {
    "proportion": {
      "numerator": [
        {
          "calculation": [{
              "formula": "(meta_timeend - dob)/86400",
              "name": "num_of_days"
          },
          {
              "formula": "(meta_timeend - last_muac)/86400",
              "name": "num_of_days_last_muac"
          }
          ],
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$lte": "{{period.end}}"
            },
            "cur_muac": {"$ne":""},
            "num_of_days": {
                "$gt": 180,
                "$lt": 1825
             },
             "num_of_days_last_muac": {
                "$lt": 30
             } 
          },
          "source": "Child List:Visit.csv"
        }
      ],
      "denominator": [
            {
            "calculation": {
              "formula": "(form_date_of_death - form_dob)/86400",
              "name": "num_of_days_alive"
            },
            "count": true,
            "dataset_id": "22af70b95bd84fa6a02098de19ac0304",
            "query": {
              "form_meta_timeend": {
                "$gte": "{{period.start}}",
                "$lt": "{{period.end}}"
              },
              "form_reason": "death",
              "num_of_days_alive": {
                "$lt": 28
              }
            },
           "source": "case_export_all.csv"
          },
          {
            "count": true,
            "dataset_id": "7b73c96e9179466b9a0cc16bce09e4f6",
            "query": {
              "closed_on": {
                "$gte": "{{period.thirty_days_before_end_of_period}}",
                "$lte": "{{period.end}}"
              },
              "opened_on": {
                "$lte": "{{period.end}}"
              },              
              "d_dob_calc": {
                "$gte": "{{period.start}}",
                "$lte": "{{period.end}}"
              },
              "closed": true,
              "type": "child"
           },
          "source": "case_export_all.csv"
          }
      ]
    }
  }
}
