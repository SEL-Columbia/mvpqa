{
  "description": "Proportion of Under-5's with complicated fever who were referred to clinic or hospital",
  "name": "proportion_of_under5_with_complicated_fever_referred",
  "type": "Malaria",
  "value": {
    "proportion": {
      "numerator": [
        {
          "calculation": {
            "formula": "(meta_timeend - dob)/86400",
            "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "$and": [
              {
                "server_computed__mvp_indicators_immediate_danger_sign_value": {
                  "$regex": "fever",
                  "$options": "i"
                }
              },
              {
                "server_computed__mvp_indicators_immediate_danger_sign_value": {
                  "$ne": "fever"
                }
              }
            ],
            "server_computed__mvp_indicators_referral_type_value": {
              "$in": [
                "basic",
                "emergency",
                "take_to_clinic",
                "convenient"
              ]
            }
          },
          "source": "Child List Visit.csv"
        }
      ],
      "denominator": [
        {
          "calculation": {
            "formula": "(meta_timeend - dob)/86400",
            "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "$and": [
              {
                "server_computed__mvp_indicators_immediate_danger_sign_value": {
                  "$regex": "fever",
                  "$options": "i"
                }
              },
              {
                "server_computed__mvp_indicators_immediate_danger_sign_value": {
                  "$ne": "fever"
                }
              }
            ]
          },
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}
