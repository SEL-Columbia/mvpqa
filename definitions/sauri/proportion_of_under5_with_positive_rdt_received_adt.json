{
  "description": "Proportion of Under-5's with positive RDT result who received antimalarial/ADT medication",
  "name": "proportion_of_under5_with_positive_rdt_received_adt",
  "type": "Malaria",
  "value": {
    "proportion": {
      "numerator": [
        {
	  "calculation":
            {
              "formula": "(meta_timeend - dob)/86400",
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "server_computed__mvp_indicators_immediate_danger_sign_value": "fever",
            "server_computed__mvp_indicators_rdt_result_value": "positive",
            "server_computed__mvp_indicators_fever_medication_value": {
                "$regex": "coartem",
                "$options": "i"
            }
          },
          "source": "Child List Visit.csv"
        }
      ],
      "denominator": [
        {
	  "calculation":
            {
              "formula": "(meta_timeend - dob)/86400",
              "name": "num_of_days"
          },
          "count": true,
          "dataset_id": "77e796170c2f423fa920e85fad166f8d",
          "query": {
            "meta_timeend": {
              "$gte": "{{period.start}}",
              "$lte": "{{period.end}}"
            },
            "num_of_days": {
              "$lt": 1825
            },
            "server_computed__mvp_indicators_immediate_danger_sign_value": "fever",
            "server_computed__mvp_indicators_rdt_result_value": "positive"
          },
          "source": "Child List Visit.csv"
        }
      ]
    }
  }
}
